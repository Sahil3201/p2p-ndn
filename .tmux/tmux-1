tmux new-session -d -s weathers
tmux new-window -n sone
tmux new-window -d -n w1temperature
tmux new-window -d -n w1humidity
tmux new-window -d -n w1wind_speed
tmux new-window -d -n w1rain_gauge
tmux new-window -d -n w1solar_radiation
tmux new-window -d -n w1evapotranspiration
tmux new-window -d -n w1atmospheric_gas
tmux new-window -d -n w1barometric_pressure
tmux send-keys -t weathers:bash "echo Base Control" Enter
tmux send-keys -t weathers:sone "python3 node.py --name /weathers/weather1" Enter
tmux send-keys -t weathers:w1temperature "python3 node.py --name /weathers/weather1/temperature" Enter
tmux send-keys -t weathers:w1humidity "python3 node.py --name /weathers/weather1/humidity" Enter
tmux send-keys -t weathers:w1wind_speed "python3 node.py --name /weathers/weather1/wind_speed" Enter
tmux send-keys -t weathers:w1rain_gauge "python3 node.py --name /weathers/weather1/rain_gauge" Enter
tmux send-keys -t weathers:w1solar_radiation "python3 node.py --name /weathers/weather1/solar_radiation" Enter
tmux send-keys -t weathers:w1evapotranspiration "python3 node.py --name /weathers/weather1/evapotranspiration" Enter
tmux send-keys -t weathers:w1atmospheric_gas "python3 node.py --name /weathers/weather1/atmospheric_gas" Enter
tmux send-keys -t weathers:w1barometric_pressure "python3 node.py --name /weathers/weather1/barometric_pressure" Enter

tmux new-session -d -s soils
tmux new-window -n sone
tmux new-window -d -n s1moisture
tmux new-window -d -n s1erosion
tmux new-window -d -n s1salinity
tmux new-window -d -n s1texture
tmux new-window -d -n s1temperature
